<%= form_with model: @auto do |form| %>
  
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>
  
  <br>
  <div>
    <%= form.label :city %><br>
    <%= form.text_field :city %>
  </div>
  
  <br>
  <div>
    <%= form.label :date %><br>
    <%= form.text_field  :date %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>